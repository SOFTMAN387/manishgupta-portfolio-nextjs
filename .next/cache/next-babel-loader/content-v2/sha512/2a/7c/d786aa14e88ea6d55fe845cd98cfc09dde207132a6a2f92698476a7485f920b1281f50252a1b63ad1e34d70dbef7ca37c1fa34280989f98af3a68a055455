{"ast":null,"code":"import { jsxs as _jsxs } from \"react/jsx-runtime\";\nimport { jsx as _jsx } from \"react/jsx-runtime\";\nimport { useState } from \"react\";\nimport ProjectCard from \"../components/ProjectCard\";\nimport ProjectsNavbar from \"../components/ProjectsNavbar\";\nimport { projects as projectsData } from \"../data\";\n\nconst Projects = () => {\n  const {\n    0: projects,\n    1: setProjects\n  } = useState(projectsData);\n  const {\n    0: active,\n    1: setActive\n  } = useState(\"all\");\n\n  const handlerFilterCategory = category => {\n    if (category === \"all\") {\n      setProjects(projectsData);\n      setActive(category);\n      return;\n    }\n\n    const newArray = projectsData.filter(project => project.category.includes(category));\n    setProjects(newArray);\n    setActive(category);\n  };\n\n  return /*#__PURE__*/_jsxs(\"div\", {\n    className: \"px-5 py-2 overflow-y-scroll\",\n    style: {\n      height: \"65vh\"\n    },\n    children: [/*#__PURE__*/_jsx(ProjectsNavbar, {\n      handlerFilterCategory: handlerFilterCategory,\n      active: active\n    }), /*#__PURE__*/_jsx(\"div\", {\n      className: \"relative grid grid-cols-12 gap-4 my-3\",\n      children: projects.map(project => /*#__PURE__*/_jsx(\"div\", {\n        className: \"col-span-12 p-2 bg-gray-200 rounded-lg sm:col-span-6 lg:col-span-4 dark:bg-dark-200\",\n        children: /*#__PURE__*/_jsx(ProjectCard, {\n          project: project\n        }, project.name)\n      }))\n    })]\n  });\n};\n\nexport default Projects;","map":null,"metadata":{},"sourceType":"module"}